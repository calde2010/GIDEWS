			<!-- <wsdlDirectory>${project.basedir}\src\main\wsdl\</wsdlDirectory> -->
			<!-- <wsdlFiles> -->
			<!-- <wsdlFile>facturaElectronica.wsdl</wsdlFile> -->
			<!-- <wsdlFile>consultaDocumentos_Habilitacion.wsdl</wsdlFile> -->
			<!-- <wsdlFile>consultaResolucionesFacturacion.wsdl</wsdlFile> -->
			<!-- <wsdlFile>facturaElectronica_Habilitacion.wsdl</wsdlFile> -->
			<!-- <wsdlFile>EstablecimientoEducativo_1.wsdl</wsdlFile> -->
			<!-- <wsdlFile>SedeEducativa_1.wsdl</wsdlFile> -->
			<!-- </wsdlFiles> -->
			<!-- <sourceDestDir>${project.basedir}\src\main\java\</sourceDestDir> -->
			<!-- <verbose>true</verbose> -->
			<!-- <vmArgs> -->
			<!-- <vmArg>-Djavax.xml.accessExternalDTD=all</vmArg> -->
			<!-- <vmArg>-Djavax.xml.accessExternalSchema=all</vmArg> -->
			<!-- </vmArgs> -->
			<!-- </configuration> -->
			<!-- <dependencies> -->
			<!-- <dependency> -->
			<!-- <groupId>com.sun.xml.ws</groupId> -->
			<!-- <artifactId>jaxws-tools</artifactId> -->
			<!-- <version>2.2.10</version> -->
			<!-- </dependency> -->
			<!-- </dependencies> -->
			<!-- </plugin> -->



			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>exec-maven-plugin</artifactId>
				<version>1.2.1</version>
				<executions>
					<execution>
						<goals>
							<goal>java</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<mainClass>GIDE_CLIENTES.App</mainClass>
				</configuration>
			</plugin>

		</plugins>



		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings 
					only. It has no influence on the Maven build itself. -->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>
											org.jvnet.jax-ws-commons
										</groupId>
										<artifactId>
											jaxws-maven-plugin
										</artifactId>
										<versionRange>
											[2.3,)
										</versionRange>
										<goals>
											<goal>wsimport</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
						<vmArgs>
							<vmArg>-Djavax.xml.accessExternalDTD=all</vmArg>
							<vmArg>-Djavax.xml.accessExternalSchema=all</vmArg>
						</vmArgs>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>


</project>
